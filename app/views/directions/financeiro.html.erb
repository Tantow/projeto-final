<body class="body-light">
  <div class="body-light-container">

    <div class="title title-dark">
      <div class="title-container">
        <h1>Financeiro</h1>

        <%# render 'form', direction: @users %>

        <table>
          <thead>
            <tr>
  						<th>Nome</th>
  						<th>Diretor</th>
  					</tr>
          </thead>
          <tbody>
              <% @users.each do |user| %>
              <tr>
                <td><%= user.full_name %></td>
    						<td><%= user.admin %></td>
              </tr>
              <% end %>
          </tbody>
        </table>
      </div>
    </div>

  </div>
</body>
